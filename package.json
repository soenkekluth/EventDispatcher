{
  "name": "eventdispatcher",
  "version": "2.5.0",
  "description": "EventDispatcher like nodes EventEmitter",
  "main": "lib/eventdispatcher.js",
  "scripts": {
    "test": "node test/index.js",
    "test:babel": "cross-env NODE_ENV=development babel-node test/index.js",
    "lint": "eslint src",
    "start": "cross-env NODE_ENV=development nodemon --exec 'npm run lint && babel-node ./src/eventdispatcher.js'",
    "build": "npm run build:lib",
    "build:all": "npm run build:lib && npm run build:dist",
    "build:lib": "cross-env NODE_ENV=production babel src -d lib",
    "build:dist": "rollup -c && rollup -c --environment PRODUCTION",
    "patch": "release patch",
    "minor": "release minor",
    "major": "release major",
    "release": "release"
  },
  "release-script": {
    "altPkgRootFolder": "lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soenkekluth/EventDispatcher.git"
  },
  "keywords": [
    "trigger",
    "dispatch",
    "event",
    "EventEmitter",
    "dispatcher",
    "eventdispatcher",
    "emit",
    "emitter",
    "observer"
  ],
  "author": "SÃ¶nke Kluth <soenke.kluth@gmail.com> (http://soenkekluth.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soenkekluth/EventDispatcher/issues"
  },
  "homepage": "https://github.com/soenkekluth/EventDispatcher",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-constructor-call": "6.18.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-es2015-classes": "6.18.0",
    "babel-plugin-transform-proto-to-assign": "6.9.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "3.1.3",
    "eslint": "3.10.2",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-plugin-import": "2.2.0",
    "nodemon": "1.11.0",
    "release-script": "1.0.2",
    "rollup": "0.36.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^6.0.1"
  },
  "dependencies": {
    "object-assign": "^4.1.0"
  }
}
